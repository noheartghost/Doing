<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ImageButtonCustom">
        <attr name="image" format="reference"></attr>
        <attr name="text" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="SubtitleCustom">
        <attr name="title" format="string"></attr>
        <attr name="sub_index" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="NavigatorBarCustom">
        <attr name="left_visible" format="boolean"></attr>
        <attr name="right_visible" format="boolean"></attr>
        <attr name="text_visible" format="boolean"></attr>
        <attr name="left_image" format="reference"></attr>
        <attr name="right_image" format="reference"></attr>
        <attr name="text_title" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="SelectTabCustom">
        <attr name="select_title" format="string"></attr>
        <attr name="select_selected" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="RecordItemCustom">
        <attr name="record_name" format="string"></attr>
        <attr name="record_content" format="string"></attr>
    </declare-styleable>

</resources>